@model IEnumerable<ApartmentManagement.Models.Apartment>

@{
    if (Model.Count() != 0)
    {
        <h5>Your Apartments</h5>
    }

    int i = 1;
    foreach (var item in Model)
    {
        <a href="/Apartments/RenterApartmentDetails/@item.apartment_id" class="list-group-item">@i- @item.Building.name AN: @item.apartment_number</a>
        i++;
    }
} 